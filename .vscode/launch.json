    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "env": {
                "ADMIN_PORT": "4100",
                "API_PORT": "4200",
                "ENABLE_BASIC_AUTH": "true",
                "ADMIN_USER": "admin",
                "ADMIN_PASS": "admin"
            }
        }
    ]
}